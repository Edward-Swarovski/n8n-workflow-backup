    {
      "parameters": {
        "jsCode": "// Use $input for modern n8n compatibility\nconst inputData = $input.first().json.data || [];\n\nconst workflows = [...inputData]\n  // Ensure IDs are strings for stable comparison\n  .sort((a, b) => String(a.id).localeCompare(String(b.id)));\n\nconst now = new Date().toISOString();\n\nreturn workflows.map(wf => {\n  // Clean filename: removes special chars, handles empty names\n  const safeName = (wf.name || 'unnamed')\n    .trim()\n    .replace(/[^a-z0-9]+/gi, '_')\n    .replace(/^_+|_+$/g, '')\n    .toLowerCase();\n\n  const backupWorkflow = {\n    ...wf,\n    _backup: {\n      backed_up_at: now,\n      source: 'n8n',\n      backup_type: 'workflow',\n    },\n  };\n\n  return {\n    json: {\n      // Added a timestamp or ID to filename ensures uniqueness\n      filename: `workflows/${safeName}_${wf.id}.json`,\n      content: JSON.stringify(backupWorkflow, null, 2),\n      workflowName: wf.name, // Helpful for downstream logging\n    }\n  };\n});"
      },
